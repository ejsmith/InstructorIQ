<?xml version="1.0" encoding="utf-8"?>
<codeSmith xmlns="http://www.codesmithtools.com/schema/csp.xsd">
  <variables>
    <add key="ConnectionString1" value="Data Source=(local);Initial Catalog=InstructorIQ;Integrated Security=True" />
  </variables>
  <propertySets>
    <propertySet name="Entity" template="..\..\..\..\PLINQO.EntityFrameworkCore\CSharp\Entity.cst">
      <property name="ContextDirectory">.\Data\</property>
      <property name="EntityDirectory">.\Data\Entities</property>
      <property name="MappingDirectory">.\Data\Mapping</property>
      <property name="TableNaming">Singular</property>
      <property name="EntityNaming">Singular</property>
      <property name="RelationshipNaming">Plural</property>
      <property name="ContextNaming">Plural</property>
      <property name="CleanExpressions">
        <stringList>
          <string>^(sp|tbl|udf|vw)_</string>
        </stringList>
      </property>
      <property name="ContextBaseClass">DbContext</property>
      <property name="IncludeEditable">True</property>
      <property name="IncludeQuery">True</property>
      <property name="QueryDirectory">.\Data\Queries</property>
      <property name="MethodPrefix">By</property>
      <property name="UniqueMethodPrefix">GetBy</property>
      <property name="MethodKeySuffix">Key</property>
      <property name="IncludeViewModels">True</property>
      <property name="ViewModelDirectory">.\Mediator\Models</property>
      <property name="ValidationDirectory">.\Mediator\Validation</property>
      <property name="ProfileDirectory">.\Mediator\Profiles</property>
      <property name="SourceDatabase">
        <connectionString>$(ConnectionString1)</connectionString>
        <providerType>SchemaExplorer.SqlSchemaProvider,SchemaExplorer.SqlSchemaProvider</providerType>
        <mode>Exclude</mode>
      </property>
      <property name="ContextNamespace">InstructorIQ.Core.Data</property>
      <property name="EntityNamespace">InstructorIQ.Core.Data.Entities</property>
      <property name="MappingNamespace">InstructorIQ.Core.Data.Mapping</property>
      <property name="QueryNamespace">InstructorIQ.Core.Data.Queries</property>
      <property name="ViewModelNamespace">InstructorIQ.Core.Mediator.Models</property>
      <property name="ValidationNamespace">InstructorIQ.Core.Mediator.Validation</property>
      <property name="ProfileNamespace">InstructorIQ.Core.Mediator.Models</property>
      <property name="RegistrationFile">.\Mediator\MediatorServiceModule.cs</property>
      <property name="IgnoredProperties">
        <stringList>
          <string>Id</string>
          <string>Created</string>
          <string>CreatedBy</string>
          <string>Updated</string>
          <string>UpdatedBy</string>
          <string>RowVersion</string>
          <string>IsDeleted</string>
          <string>User.PasswordHash</string>
          <string>User.SecurityStamp</string>
        </stringList>
      </property>
      <property name="IgnoredEntities">
        <stringList>
          <string>RefreshToken</string>
          <string>Role</string>
          <string>UserLogin</string>
          <string>UserRole</string>
        </stringList>
      </property>
    </propertySet>
  </propertySets>
</codeSmith>